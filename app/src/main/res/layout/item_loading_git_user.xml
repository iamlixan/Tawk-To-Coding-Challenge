                app:layout_constraintStart_toEndOf="@id/ivAvatar5"
                app:layout_constraintTop_toTopOf="@id/ivAvatar5"
                app:layout_constraintEnd_toStartOf="@id/midVerticalGuideline"
                />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/ivAvatar6"
                android:layout_width="@dimen/_80dp"
                android:layout_height="@dimen/_80dp"
                android:src="@color/gray_chateau"
                android:layout_marginTop="@dimen/_8dp"
                android:paddingTop="@dimen/_8dp"
                android:paddingBottom="@dimen/_8dp"
                app:civ_circle_background_color="@color/geyser"
                app:civ_border_color="@color/geyser"
                app:layout_constraintStart_toEndOf="@id/leftVerticalGuideline"
