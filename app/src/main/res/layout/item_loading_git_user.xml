                android:paddingBottom="@dimen/_8dp"
                app:civ_circle_background_color="@color/geyser"
                app:civ_border_color="@color/geyser"
                app:layout_constraintStart_toEndOf="@id/leftVerticalGuideline"
                app:layout_constraintTop_toBottomOf="@id/ivAvatar4"
                />

